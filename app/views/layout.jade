include mixin/navbar-links
doctype html
html
    head
        title= title
        link(rel='stylesheet' , href='../assets/style/bootstrap-paper-theme.css')
        link(rel='stylesheet', href='../assets/bower/animate.css/animate.css')
        link(rel='stylesheet',href='../assets/style/main.css')
    body
        .navbar.navbar-inverse
            .container-fluid
                .navbar-header
                    button.navbar-toggle.collapsed(type="button",data-toggle="collapse" data-target="#appNavBar")
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a.navbar-brand(ui-sref='main') Brand
                .navbar-collapse.collapse(id="appNavBar")
                    ul.nav.navbar-nav
                        +createLink(state,'/','Home')
                        +createLink(state,'/generator','Generator')


        block content
        include _partials/scripts
        block additionalScripts
